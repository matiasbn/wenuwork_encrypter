{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "add-module-exports",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-async-to-generator"
  ],
  "env": {
    "production": {
      "presets": [
        ["minify", {
          "keepClassName": false,
          "keepFnName": false,
          "removeConsole": true,
          "removeDebugger": true
        }]
      ]
    }
  }
}
